{
  "class_name": "CalculatorTests",
  "test_cases": [
    {
      "name": "addition",
      "description": "Test addition operation with positive numbers",
      "setup": "calculator = Calculator()",
      "assertions": [
        "result = calculator.add(2, 3)",
        "assert result == 5"
      ]
    },
    {
      "name": "subtraction",
      "description": "Test subtraction operation",
      "setup": "calculator = Calculator()",
      "assertions": [
        "result = calculator.subtract(10, 5)",
        "assert result == 5"
      ]
    },
    {
      "name": "multiplication",
      "description": "Test multiplication operation",
      "setup": "calculator = Calculator()",
      "assertions": [
        "result = calculator.multiply(4, 5)",
        "assert result == 20"
      ]
    },
    {
      "name": "division",
      "description": "Test division operation",
      "setup": "calculator = Calculator()",
      "assertions": [
        "result = calculator.divide(10, 2)",
        "assert result == 5"
      ]
    },
    {
      "name": "division_by_zero",
      "description": "Test division by zero raises exception",
      "setup": "calculator = Calculator()",
      "assertions": [
        "with pytest.raises(ZeroDivisionError):",
        "    calculator.divide(10, 0)"
      ]
    }
  ]
}
